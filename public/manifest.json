{
    "manifest_version": "ext-v1",
    "name": "TLDraw",
    "version": "1.0.0",
    "version_name": "Initial",
    "short_name": "tldraw",
    "description": "TLDraw is a collaborative digital whiteboard.",
    "author": "EXT, Inc",
    "homepage_url": "https://ext.store",
    "icons": {
        "128": "icon.png",
        "128-dark": "icon-dark.png",
        "banner": "icon-banner.png",
        "banner-dark": "icon-banner-dark.png",
        "background": "icon-background.png",
        "background-dark": "icon-background-dark.png"
    },
    "scripts": [
        "main.js"
    ],
    "modules": {
        "console": {},
        "runtime": {
            "optional": false,
            "permissions": [
                "reload"
            ]
        },
        "tabs": {},
        "timers": {},
        "windows": {},
        "websessions": {},
        "webviews": {}
    }
}